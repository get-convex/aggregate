{
  "name": "examples",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite --open",
    "dev:backend": "convex dev --live-component-sources --typecheck-components",
    "dev:ts": "tsc -b -w",
    "predev": "convex dev --until-success && convex dashboard",
    "build": "tsc -b && vite build",
    "lint": "tsc && eslint .  --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@convex-dev/aggregate": "file:..",
    "@convex-dev/migrations": "^0.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mantine/core": "^8.2.2",
    "@mantine/emotion": "^8.2.2",
    "@mantine/hooks": "^8.2.2",
    "@mantine/notifications": "^8.2.2",
    "@tabler/icons-react": "^3.34.1",
    "@xyflow/react": "^12.8.2",
    "convex": "^1.17.0",
    "convex-helpers": "^0.1.61",
    "rand-seed": "^3.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "type-route": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "npm-run-all2": "8.0.4",
    "prettier": "^3.5.3",
    "typescript-eslint": "8.44.0",
    "vite": "^6.3.5",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.34.0",
    "@typescript-eslint/eslint-plugin": "8.44.0",
    "@typescript-eslint/parser": "8.44.0",
    "convex-test": "^0.0.38",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "typescript": "^5.9.2"
  }
}
